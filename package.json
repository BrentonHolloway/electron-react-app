{
  "name": "electron-react-app",
  "version": "0.1.0",
  "productName": "Electron React Application",
  "description": "Electron Application using React as the front end.",
  "author": {
    "name": "Brenton Holloway"
  },
  "build": {
    "appId": "electron.react.app",
    "extraMetadata": {
      "homepage": "./",
      "main": "build/electron/main.js"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "build/logo512.png"
    }
  },
  "private": true,
  "home": "./",
  "homepage": "./",
  "main": "./public/electron/main.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.4.0",
    "concurrently": "^5.3.0",
    "electron-is-dev": "^1.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "wait-on": "^5.2.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "dev": "concurrently \"npm run react-start\" \" wait-on http://localhost:3000 && electron .\"",
    "pack": "electron-builder --dir",
    "release": "react-build && electron-builder --publish=always",
    "build": "npm run react-build && electron-builder build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1"
  }
}
